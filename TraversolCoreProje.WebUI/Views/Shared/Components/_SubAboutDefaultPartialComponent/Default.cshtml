@model List<ResultSubAboutViewModel>
<section class="w3l-bottom py-5">
    <div class="container py-md-4 py-3 text-center">
        <div class="row my-lg-4 mt-4">
            <div class="col-lg-9 col-md-10 ml-auto">
                <div class="bottom-info ml-auto">
                    @if (Model is { Count: > 0 })
                    {
                        @foreach (var item in Model)
                        {
                            <div class="header-section text-left">
                                <h3 class="hny-title two">@item.Title</h3>
                                <p class="mt-3 pr-lg-5">
                                    @item.Description
                                </p>
                                <a href="about.html" class="btn btn-style btn-secondary mt-5">Detaya Git</a>
                            </div>
                        }
                    }
                    else
                    {
                        <div class="alert alert-warning text-center">
                            <p>@ViewBag.Error</p>
                        </div>
                    }

                </div>
            </div>
        </div>
    </div>
</section>